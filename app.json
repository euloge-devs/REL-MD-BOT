{
  "name": "OVL-MD-V2",
  "description": "Bot WhatsApp Multi-Device basé sur Baileys",
  "keywords": ["Rel-Md,Whatsapp,Baileys,Multi-device,Bot,Koyeb,Zeabur"],
  "logo": "https://files.catbox.moe/4vhd5w.png",
  "repository": "https://github.com/euloge-devs/REL-MD-BOT",
  "success_url": "/",
  "stack": "container",
  "env": {
    "PREFIXE": {
      "description": "Préfixe utilisé pour les commandes du bot",
      "value": ".",
      "required": false
    },
    "NOM_OWNER": {
      "description": "Nom de l'Owner du bot",
      "value": "Euloge-devs",
      "required": false
    },
    "NUMERO_OWNER": {
      "description": "Numéro WhatsApp de l'Owner (au format international)",
      "value": "229xxxxxxxx",
      "required": true
    },
    "MODE": {
      "description": "Mode de fonctionnement du bot (public ou private)",
      "value": "public",
      "required": false
    },
    "SESSION_ID": {
      "description": "Identifiant de session",
      "value": "rel",
      "required": true
    },
    "DATABASE": {
      "description": "Lien de la base de données PostgreSQL",
      "value": "",
      "required": false
    },
    "LEVEL_UP": {
      "description": "Activer le système de niveau (oui ou non)",
      "value": "non",
      "required": false
    },
    "STICKER_PACK_NAME": {
      "description": "Nom du pack de stickers",
      "value": "Wa-sticker",
      "required": false
    },
    "STICKER_AUTHOR_NAME": {
      "description": "Auteur du sticker",
      "value": "rel-md",
      "required": false
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
